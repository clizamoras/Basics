
theboard={'11':'','12':'','13':'','21':'','22':'','23':'','31':'','32':'','33':''}
def tboard(theboard):
  print(theboard['11'],'|',theboard['12'],'|',theboard['13'])
  print('--++--')
  print(theboard['21'],'|',theboard['22'],'|',theboard['33'])
  print('--++--')
  print(theboard['31'],'|',theboard['32'],'|',theboard['33'])
tboard(theboard)

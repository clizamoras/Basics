import os
lines = open("C:\\Users\\Lenovo\\OneDrive\\Desktop\\Sort.txt", "r").read() # reading file
print(lines)
l = lines.split(' ') # making list
print(l)
l.sort() # sorting the list
print(l)
sFile = open('F:\\Sort.txt', 'w')
for i in l:
sFile.write(i )
sFile.write(' ')
sFile.close()


class Solution:
 def symmetry(self,root):
  def mirror(a,b):
   if not a and not b:
     return True
   if not a or not b:
     return False
   if a.val!=b.val:
     return False
   return mirror(a.left,b.right) and mirror(a.right,b.left)
 if not root:
  return True
return mirror(root.left,root.right)

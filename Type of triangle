
a=float(input('Enter the sides of a triangle'))
b=float(input('Enter the sides of a triangle'))
c=float(input('Enter the sides of a triangle'))
if c**2==a**2+b**2 or a**2==b**2+c**2 or b**2==a**2+c**2:
  print('It is a right angled triangle')
elif a-b==c:
  print('It is an equilateral triangle')
else :
  print('It is none of them')

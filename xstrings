def isUSN(text):
  if len(text)!=10:
    return False
  if not text[0].isdecimal():
    return False
  for i in range(1,3):
    if not text[i].isalpha():
      return False
  for i in range(3,5):
    if not text[i].isdecimal():
      return False
  for i in range(5,7):
    if not text[i].isalpha():
      return False
  for i in range(7,10):
     if not text[i].isdecimal():
      return False
  return True
print(isUSN('4SO24CS029'))
